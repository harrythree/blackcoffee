{
  "expo": {
    "name": "mobile",
    "slug": "mobile",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "package": "com.harrybaldwin.blackcoffee",
      "config": {
        "googleMaps": {
          "apiKey": "<android maps key>"
        }
      }
    },
    "ios": {
      "bundleIdentifier": "com.harrybaldwin.blackcoffee",
      "config": {
        "googleMapsApiKey": "<ios map api key>"
      }
    }
  }
}
